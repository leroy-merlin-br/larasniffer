{
    "name": "leroy-merlin-br/larasniffer",
    "description": "Larasniffer detect violations of a defined coding standard. It helps your code remains clean and consistent.",
    "keywords": ["laravel","sniff","psr2","psr1"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zizaco Ziuini",
            "email": "zizaco@gmail.com",
            "homepage": "http://www.zizaco.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "*",
        "illuminate/console": "*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*@dev"
    },
    "autoload": {
        "psr-0": {
            "LeroyMerlin\\LaraSniffer\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
